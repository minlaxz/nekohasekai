{
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
      "external_ui": "metacubexd",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "hysteria-out",
      "secret": "123123123",
      "default_mode": "cf-proxied"
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "cloudflare-doh-dns",
        "address": "8.8.8.8",
        "detour": "direct-out"
      },
      {
        "tag": "next-doh-dns",
        "address": "https://dns.nextdns.io/627afc",
        "address_resolver": "cloudflare-direct-dns",
        "detour": "direct-out"
      },
      {
        "tag": "cloudflare-direct-dns",
        "address": "1.1.1.1",
        "detour": "direct-out",
        "address_strategy": "ipv4_only"
      },
      {
        "tag": "next-direct-dns",
        "address": "45.90.28.18",
        "detour": "direct-out",
        "address_strategy": "ipv4_only"
      },
      {
        "tag": "adguard-direct-dns",
        "address": "94.140.14.14",
        "detour": "direct-out",
        "address_strategy": "ipv4_only"
      },
      {
        "tag": "dns-blocked",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "clash_mode": "ag-nothing",
        "server": "adguard-direct-dns"
      },
      {
        "clash_mode": "cf-proxied",
        "server": "cloudflare-doh-dns"
      },
      {
        "clash_mode": "nx-proxied",
        "server": "next-doh-dns"
      },
      {
        "domain_suffix": ["openai.com", "top10vpn.com"],
        "server": "cloudflare-doh-dns"
      }
    ],
    "final": "adguard-direct-dns",
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "tun0",
      "inet4_address": "172.28.1.0/24",
      "auto_route": true,
      "strict_route": true,
      "stack": "mixed",
      "sniff": true
    }
  ],
  "outbounds": [
    {
      "type": "hysteria2",
      "tag": "hysteria-out",
      "server": "your-server-ip-or-domain",
      "server_port": 19443,
      "up_mbps": 100,
      "down_mbps": 100,
      "password": "123123123",
      "tls": {
        "enabled": true,
        "server_name": "anything.yourdomain.tld"
      }
    },
    {
      "type": "direct",
      "tag": "direct-out"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    }
  ],
  "route": {
    "geoip": {
      "download_url": "https://github.com/SagerNet/sing-geoip/releases/latest/download/geoip.db",
      "download_detour": "hysteria-out"
    },
    "geosite": {
      "download_url": "https://github.com/SagerNet/sing-geosite/releases/latest/download/geosite.db",
      "download_detour": "hysteria-out"
    },
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "clash_mode": "ag-nothing",
        "outbound": "direct-out"
      },
      {
        "clash_mode": "cf-proxied",
        "outbound": "hysteria-out"
      },
      {
        "clash_mode": "nx-proxied",
        "outbound": "hysteria-out"
      },
      {
        "geoip": ["private"],
        "outbound": "direct-out"
      }
    ],
    "final": "hysteria-out",
    "auto_detect_interface": true
  }
}
