{
    "log": {
        "loglevel": "debug"
    },
    "inbounds": [
        {
            "port": 1443, 
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "", // generate using `xray uuid`
                        "flow": "xtls-rprx-vision"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "tcp",
                "security": "reality",
                "realitySettings": {
                    "dest": "1.1.1.1:443",
                    "serverNames": [
                      ""
                    ],
                    "privateKey": "", // generate using `xray x25519`
                    "shortIds":[
                        "0123456789abcdef" // anything 0 to f, max 16
                    ]
                }
            },
            "sniffing": {
                "enabled": true,
                "destOverride": [
                    "http",
                    "tls",
                    "quic"
                ],
                "routeOnly": true
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom",
            "tag": "direct"
        }
    ]
}
